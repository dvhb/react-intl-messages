{
  "name": "@dvhb/react-intl-messages",
  "description": "Library for parsing source files and extract react-intl messages",
  "version": "0.0.1",
  "author": "Anton Savoskin @sairus2k",
  "bin": {
    "messages": "./bin/run"
  },
  "bugs": "https://github.com/sairus2k/react-intl-messages/issues",
  "dependencies": {
    "@babel/core": "^7.2.2",
    "@oclif/command": "^1.5.10",
    "@oclif/config": "^1.12.6",
    "@oclif/plugin-help": "^2.1.6",
    "babel-plugin-react-intl": "^3.0.1",
    "cosmiconfig": "^5.1.0",
    "dotenv": "^7.0.0",
    "glob": "^7.1.3",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@dvhb/tslint-config": "^1.0.6",
    "@oclif/dev-cli": "^1.21.0",
    "@oclif/test": "^1.2.4",
    "@types/babel__core": "^7.0.5",
    "@types/chai": "^4.1.7",
    "@types/cosmiconfig": "^5.0.3",
    "@types/dotenv": "^6.1.1",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.12.26",
    "@types/request-promise-native": "^1.0.15",
    "chai": "^4.2.0",
    "globby": "^8.0.2",
    "mocha": "^5.2.0",
    "nyc": "^13.3.0",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.1",
    "typescript": "^3.3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "keywords": [
    "react",
    "react-intl",
    "lokalise.co"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "messages",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "printWidth": 120
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:dvhb/react-intl-messages.git"
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
