{
  "name": "@dvhb/react-intl-messages",
  "version": "2.2.2",
  "description": "Library for parsing source files and extract react-intl messages",
  "keywords": [
    "lokalise.co",
    "react",
    "react-intl"
  ],
  "bugs": "https://github.com/sairus2k/react-intl-messages/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/dvhb/react-intl-messages.git"
  },
  "license": "MIT",
  "author": "Anton Savoskin @sairus2k",
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "messages": "./bin/run"
  },
  "scripts": {
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "postpack": "rm -f oclif.manifest.json",
    "semantic-release": "semantic-release",
    "test": "jest",
    "posttest": "tslint -p . -t stylish",
    "version": "oclif-dev readme && git add README.md",
    "lint": "tslint -p tsconfig.json -c tslint.json -t verbose"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "jest": {
    "preset": "ts-jest",
    "setupFiles": [
      "<rootDir>/test/setup.js"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "<rootDir>/test/**/*.test.js"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.4.5",
    "@oclif/command": "^1.5.14",
    "@oclif/config": "^1.13.0",
    "@oclif/plugin-help": "^2.2.0",
    "babel-plugin-react-intl": "^3.3.3",
    "cosmiconfig": "^5.2.1",
    "dotenv": "^8.0.0",
    "glob": "^7.1.4",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@dvhb/tslint-config": "^1.0.9",
    "@oclif/dev-cli": "^1.22.0",
    "@oclif/test": "^1.2.4",
    "@types/babel__core": "^7.1.2",
    "@types/cosmiconfig": "^5.0.3",
    "@types/dotenv": "^6.1.1",
    "@types/glob": "^7.1.1",
    "@types/nock": "^10.0.3",
    "@types/node": "^12.0.8",
    "babel-preset-react-app": "^9.0.0",
    "globby": "^9.2.0",
    "husky": "^2.4.1",
    "jest": "^24.8.0",
    "nock": "^10.0.6",
    "semantic-release": "^15.13.16",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.17.0",
    "typescript": "^3.5.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "oclif": {
    "commands": "./lib/commands",
    "bin": "messages",
    "plugins": [
      "@oclif/plugin-help"
    ]
  }
}
